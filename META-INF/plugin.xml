<idea-plugin version="2">
  <id>com.TestPlaginJava2UM</id>
  <name>TestPlaginJava2UML</name>
  <version>0.1</version>
  <vendor url="http://www.java2uml.ru" email="java2uml@yandex.ru">Java2UML open-source team</vendor>


  <description><![CDATA[
     <!--<img src="../com/github/java2uml/plugin/idea/icon/logo.png" alt="Java2UML">-->
     <img src="http://2.bp.blogspot.com/-J0Jda7a9lsk/VNihGKtTH6I/AAAAAAAAAck/A29-H-rQk3A/s1600/logo5_long_700w_300dpi.png" alt="Java2UML"
     width="275" height="100">
     <br>
     A user-friendly automatic<br> builder of beautiful classes UML <br> diagrams based on Java source code.<br><br>
    ]]></description>

  <change-notes><![CDATA[
      <p>0.1</p>
      <ul>
          <li>Initial release of the TestJava2UMLPlugin <br>(uses Java2UML core v. 0.4a)</li>
      </ul>
    ]]>
  </change-notes>

  <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
  <idea-version since-build="8000"/>

  <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
       on how to target different products -->
  <!-- uncomment to enable plugin in all products-->
  <depends>com.intellij.modules.lang</depends>


  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <toolWindow id="Java2UML" secondary="false" anchor="right" factoryClass="com.github.java2uml.plugin.idea.MainPanel"
                icon="/com/github/java2uml/plugin/idea/icon/logo.png"></toolWindow>

  </extensions>

  <application-components>
    <!-- Add your application components here -->
    <component>
      <implementation-class>com.github.java2uml.plugin.idea.Java2UMLPluginRegistration</implementation-class>
    </component>
  </application-components>

  <project-components>
    <!-- Add your project components here -->
  </project-components>

  <actions>
    <!-- Add your actions here -->
    <group id="Java2UML.Uzel" icon="/com/github/java2uml/plugin/idea/icon/logo.png" text="Java2UML" description="Java2UML toolbar" popup="true">
        <add-to-group group-id="ToolsMenu" anchor="first"/>
    </group>
    <group id="Java2UML.Toolbar" description="Java2UML toolbar" text="Java2UML">
        <add-to-group group-id="Java2UML.Uzel" anchor="first"/>
      <action id="Java2UML.Generate" description="Generate diagram"  icon="/actions/execute.png"
              class="com.github.java2uml.plugin.idea.actions.GenerateAction" text="Generate diagram for chosen files"/>
      <action id="Java2UML.FileChoose" description="Choose file for generating diagram"  icon="/actions/menu-open.png"
              class="com.github.java2uml.plugin.idea.actions.FileChooseAction" text="Choose file for generating diagram"/>
        <action id="Java2UML.openOnPlantUMLServer" description="open diagram on PlantUML server"  icon="/com/github/java2uml/plugin/idea/icon/webModule.png"
                class="com.github.java2uml.plugin.idea.actions.openOnPlantUMLServerAction" text="open diagram on PlantUML server"/>
        <action id="Java2UML.SaveAs" description="Save diagram as"  icon="/actions/menu-saveall.png"
              class="com.github.java2uml.plugin.idea.actions.SaveAsAction" text="Save image as"/>
        <action id="Java2UML.OpenDiagramInViewer" description="Open diagram in Viewer"  icon="/actions/preview.png"
                class="com.github.java2uml.plugin.idea.actions.OpenDiagramInViewerAction" text="Open diagram in Viewer"/>
      <separator/>
        <action id="Java2UML.Copy" description="Copy PlantUML code" class="com.github.java2uml.plugin.idea.actions.CopyAction"
                icon="/actions/copy.png" text="Copy PlantUML code"/>
        <action id="Java2UML.Clear" description="Clear PlantUML code field" class="com.github.java2uml.plugin.idea.actions.ClearAction"
                icon="/actions/forceRefresh.png" text="Clear PlantUML code field"/>
        <action id="Java2UML.Cancel" description="Cancel" class="com.github.java2uml.plugin.idea.actions.CancelAction"
                icon="/actions/cancel.png" text="Cancel"/>
      <separator/>
      <action id="Java2UML.Settings" description="Configure Java2UML"  icon="/com/github/java2uml/plugin/idea/icon/Configure.png"
              class="com.github.java2uml.plugin.idea.actions.SettingsAction" text="Configure Java2UML"/>
      <separator/>
      <action id="Java2UML.Help" description="Help (full)"  icon="/actions/help.png"
              class="com.github.java2uml.plugin.idea.actions.HelpAction" text="Help (full)">
      <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt F1"/>
      </action>
      <action id="Java2UML.QuickHelp" description="Quick Help (tutorial)" icon="/actions/createFromUsage.png"
                class="com.github.java2uml.plugin.idea.actions.QuickHelpAction" text="Quick Help (tutorial)"/>
      <action id="Java2UML.About" description="About" icon="/com/github/java2uml/plugin/idea/icon/logo.png"
              class="com.github.java2uml.plugin.idea.actions.AboutAction" text="About"/>
    </group>
    <action id="Java2UML.GenerateDiagramForCurrentProject" icon="/actions/erDiagram.png"
            class="com.github.java2uml.plugin.idea.actions.GenerateDiagramForCurrentProjectAction"
            text="Generate Diagram For Current Project" description="Generate Diagram For Current Project">
      <!--<add-to-group group-id="ProjectViewPopupMenuSettings" anchor="first"/>-->
      <add-to-group group-id="Java2UML.Toolbar" anchor="first"/>
      <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt ENTER" second-keystroke="ctrl alt F9"/>
    </action>
  </actions>

</idea-plugin>